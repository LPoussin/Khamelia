{% block body %}
<div class="row">
    <div class="col-md-6 col-lg-12 col-xl-6">
        <section class="panel">
            <header class="panel-heading">
                <div class="panel-actions">
                    <a href="{{ path('enseigne') }}" class="">
                        <button class="mb-xs mt-xs mr-xs btn btn-primary">
                            Retour
                        </button>
                    </a>
                    <a href="#" class="fa fa-caret-down"></a>
                    
                </div>
        
                <h2 class="panel-title">
                  Cr√©er une nouvelle enseigne
                </h2>
                
            </header>
            <div class="panel-body">
               <form action="{{ path("new_enseigne") }}" class="form-horizontal" method="post">
               		<div class="form-group">
               			<label class="col-md-3 control-label" for="nom_enseigne">Nom de l'enseigne</label>
                        <div class="col-md-6">
               				<input type="text" class="form-control" name="nom_enseigne" id="nom_enseigne">
               			</div>	
               		</div>
                    <div class="form-group">
               			<label class="col-md-3 control-label" for="code_enseigne">Code de l'enseigne</label>
                        <div class="col-md-6">
               				<input type="text" class="form-control" name="code_enseigne" id="code_enseigne">
               			</div>	
               		</div>
               		
                    <input type="hidden" value="{{ csrf_token('new-enseigne') }}" 
                        name="_token">
                    <input type="submit" value="Valider" class="mb-xs mt-xs mr-xs btn btn-success">
               </form>
            </div>
        </section>
    </div>
</div>

