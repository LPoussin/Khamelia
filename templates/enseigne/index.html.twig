{% extends 'base.html.twig' %}

{% block title %}KhaMELIA - Enseigne{% endblock %}
{% block stylesheets %}
	
	<link rel="stylesheet" href="{{ asset('assets/vendor/select2/select2.css') }}" />
	<link rel="stylesheet" href="{{ asset('assets/vendor/jquery-datatables-bs3/assets/css/datatables.css') }} " />
	<link rel="stylesheet" href="{{ asset('assets/vendor/pnotify/pnotify.custom.css') }} " />

	<link rel="stylesheet" href="{{ asset('assets/vendor/jquery-ui/css/ui-lightness/jquery-ui-1.10.4.custom.css') }}" />
	<link rel="stylesheet" href="{{ asset('assets/vendor/select2/select2.css') }}" />
	<link rel="stylesheet" href="{{ asset('assets/vendor/bootstrap-multiselect/bootstrap-multiselect.css') }}" />
	<link rel="stylesheet" href="{{ asset('assets/vendor/bootstrap-tagsinput/bootstrap-tagsinput.css') }}" />
	<link rel="stylesheet" href="{{ asset('assets/vendor/bootstrap-colorpicker/css/bootstrap-colorpicker.css') }}" />
	<link rel="stylesheet" href="{{ asset('assets/vendor/bootstrap-timepicker/css/bootstrap-timepicker.css') }}" />
	<link rel="stylesheet" href="{{ asset('assets/vendor/dropzone/css/basic.css') }}" />
	<link rel="stylesheet" href="{{ asset('assets/vendor/dropzone/css/dropzone.css') }}" />
	<link rel="stylesheet" href="{{ asset('assets/vendor/bootstrap-markdown/css/bootstrap-markdown.min.css') }}" />
	<link rel="stylesheet" href="{{ asset('assets/vendor/summernote/summernote.css') }}" />
	<link rel="stylesheet" href="{{ asset('assets/vendor/summernote/summernote-bs3.css') }}" />
	<link rel="stylesheet" href="{{ asset('assets/vendor/codemirror/lib/codemirror.css') }}" />
	<link rel="stylesheet" href="{{ asset('assets/vendor/codemirror/theme/monokai.css') }}" />

{% endblock %}

{% block position %}
	{{ position }}
{% endblock %}

{% block chemin %}
	{{ chemin }}
{% endblock %}

{% block body %}
	<div class="row">
		<div class="col-md-6 col-lg-12 col-xl-6">
			<section class="panel">
				<header class="panel-heading">
					<div class="panel-actions">
						{% if app.user.type == TYPE_ENTREPRISE %}
							<a href="{{ path('new_enseigne') }}" class="">
								<button class="mb-xs mt-xs mr-xs btn btn-success">
									<span class="fa fa-plus"></span>
									Créer
								</button>
								
							</a>
						{% endif %}
						<a href="#" class="fa fa-caret-down"></a>
					</div>
			
					<h2 class="panel-title">
						{% if app.user.type == TYPE_PARTICULIER %}
							Enseignes de KhaMELIA
						{% elseif app.user.type == TYPE_ENTREPRISE %}
							Vos enseignes créées
						{% endif %}
					</h2>
					
				</header>
				<div class="panel-body">
					<table class="table table-bordered table-striped mb-none" id="datatable-default">
						<thead>
							<tr>
								<th class="center">Nom de l'enseigne</th>
								<th class="center">Code de l'enseigne</th>
								<th class="center">Action(s)</th>
							</tr>
						</thead>
						<tbody>
							{% if app.user.type == TYPE_PARTICULIER %}
								{% include 'enseigne/particulierEnseignesListe.html.twig' %}
							{% elseif app.user.type == TYPE_ENTREPRISE %}
								{% for enseigne in enseignes %}
									<tr class="gradeX center">
										<td>
											{{ enseigne.nom_enseigne }}
										</td>
										<td>
											{{ enseigne.code_enseigne }}
										</td>
										<td class="center">
										{% if enseigne.id_enseigne not in affiliées %}
											<a href="#modalCenter-{{ enseigne.id_enseigne }}" 
												class="modal-basic mb-xs mt-xs mr-xs btn btn-success">
												<span class="fa fa-plus "></span>
													Affilier
											</a>

											<div id="modalCenter-{{ enseigne.id_enseigne }}" 
												class="modal-block modal-block-primary mfp-hide">
												<section class="panel">
													<header class="panel-heading">
														<h2 class="panel-title">Attention</h2>
													</header>
													<div class="panel-body text-center">
														<div class="modal-wrapper">
															<div class="modal-icon center">
																<i class="fa fa-question-circle"></i>
															</div>
															<div class="modal-text">
																<h4>Attention</h4>
																<p>Êtes-vous sur de vouloir affilier cette enseigne à 
																KhaMELIA?</p>
															</div>
														</div>
													</div>
													<footer class="panel-footer">
														<div class="row">
															<div class="col-md-12 text-right">
																<a href="{{ path('affilier', {'id_enseigne': enseigne.id_enseigne}) }}">
																	<button class="btn btn-primary">Oui</button>
																</a>
																<button class="btn btn-default modal-dismiss">Non</button>
															</div>
														</div>
													</footer>
												</section>
											</div>
										{% else %}
											<a href="{{ path('membre', {'id_enseigne': enseigne.id_enseigne}) }} " class="mb-xs mt-xs mr-xs btn btn-info">
												<span class="fa fa-users"></span>
												Membres
											</a>
											{# Session for inscription if it's a secratire #}
											{% for value in onlineUserProfiles %}
												{% if value.IdEnseigne == enseigne.id_enseigne and 'secretaire' in value.profiles  %}
													<a href="{{ path('membre_inscription', {'id_enseigne': enseigne.id_enseigne}) }}" class="mb-xs mt-xs mr-xs btn btn-info">
														<span class="fa fa-info"></span>
														Inscriptions
													</a>
												{% endif %}
											{% endfor %}
											{# Type enseigne #}
											<a href="#modalFormType-{{ enseigne.id_enseigne }}" class="mb-xs mt-xs mr-xs btn btn-dark modal-with-form ">
												<span class="fa fa-cube"></span>
												Type enseigne
											</a>
											<div id="modalFormType-{{ enseigne.id_enseigne }}" class="modal-block modal-block-primary mfp-hide">
												<section class="panel">
													<header class="panel-heading">
														<h2 class="panel-title">Ajout de type d'enseigne</h2>
													</header>
													<div class="panel-body">
														<form action="{{ path('add_type') }} " method="post">
															<input type="hidden" name="_token" value="{{ csrf_token('type-item'~ app.user.id) }}"/>
															<input type="hidden" value="{{ enseigne.id_enseigne }}" name="id_enseigne">
															<div class="form-group mt-lg">
																<label class="col-sm-3 control-label">Type d'enseigne</label>
																<div class="col-sm-9">
																	<select class="form-control" multiple="multiple" data-plugin-multiselect data-plugin-options='{ "enableCaseInsensitiveFiltering": true }' id="ms_example6" name="types[]" required="required">
																		{% for ensTypeEns in TypeEns %}

																			<option 
																			value="{{ ensTypeEns.id }}" 
																		{% for val in typeEnsArray[enseigne.id_enseigne] %}
																			{% if ensTypeEns.id == val.idTypeEns  %}
																				selected="selected" 
																			{% endif %}
																		{% endfor %}
																			
																			
																				>{{ ensTypeEns.libelle }}</option>
																		{% endfor %}
																		</optgroup>
																	</select>
																</div>									
															</div>
															<div class="form-group text-right pr-md">
																<input type="submit" value="Ajouter" class="btn btn-primary">
															</div>
														</form>
													</div>
													<footer class="panel-footer">
														<div class="row">
															<div class="col-md-12 text-right">
																<button class="btn btn-default modal-dismiss">Cancel</button>
															</div>
														</div>
													</footer>
												</section>
											</div>
											{# Niveau d'étude #}
											
											<div id="modalFormNiveau-{{ enseigne.id_enseigne }}" class="modal-block modal-block-primary mfp-hide">
												<section class="panel">
													<header class="panel-heading">
														<h2 class="panel-title">Ajout de type d'enseigne</h2>
													</header>
													<div class="panel-body">
														<form action="{{ path('ajoute_niveau') }} " method="post">
															<input type="hidden" name="_token" value="{{ csrf_token('niveau-item'~ app.user.id) }}"/>
															<input type="hidden" value="{{ enseigne.id_enseigne }}" name="id_enseigne">
															<div class="form-group mt-lg">
																<label class="col-sm-3 control-label">Type d'enseigne</label>
																<div class="col-sm-9">
																	<select class="form-control" multiple="multiple" data-plugin-multiselect data-plugin-options='{ "enableCaseInsensitiveFiltering": true }' id="ms_example6" name="niveaux[]" required="required">
																		{% for niveauEtude in allNiveauxEtudes %}
																			{% for val in typeEnsArray[enseigne.id_enseigne] %}
																				{% if niveauEtude.typeEnseigne.id == val.idTypeEns %}
																					<option 
																					value="{{ niveauEtude.id }}" 
																					{% if niveauxEtudes[enseigne.id_enseigne] is not empty and niveauEtude.id in niveauxEtudes[enseigne.id_enseigne].niveaux %}
																						selected="selected" 
																					{% endif %}
																						>
																						{{ niveauEtude.libelle }}
																					</option>
																				{% endif %}
																			{% endfor %}
																		{% endfor %}
																	</select>
																</div>									
															</div>
															<div class="form-group text-right pr-md">
																<input type="submit" value="Ajouter / Mettre à jour" class="btn btn-primary">
															</div>
														</form>
													</div>
													<footer class="panel-footer">
														<div class="row">
															<div class="col-md-12 text-right">
																<button class="btn btn-default modal-dismiss">Cancel</button>
															</div>
														</div>
													</footer>
												</section>
											</div>
											<br>
											<a href="{{ path('matieresEnseigne', {'id_enseigne': enseigne.id_enseigne}) }} " class="mb-xs mt-xs mr-xs btn btn-warning">
												<span class="fa fa-file"></span>
												Spécialités
											</a>	
											<a href="{{ path('invitaion_medecin', {'id_enseigne' : enseigne.id_enseigne }) }} " class="mb-xs mt-xs mr-xs btn btn-danger">
												<span class="fa fa-cubes"></span>
												Médécins
											</a>
											<a href="{{ path('colaborateur', {'id_enseigne' : enseigne.id_enseigne }) }} " class="mb-xs mt-xs mr-xs btn btn-default">
												<span class="fa fa-cubes"></span>
												Collaborateurs
											</a>
											<a href="https://centrale.magmaerp.online/public/index.php/welo?querer={{userid}}&id={{enseigne.id_enseigne}}" class=" mt-xs mr-xs btn btn-dark"  target="_blank">
												<span class="fa fa-cube"></span>
												MAGMAERP
											</a>
										{% endif %}
											<a href="#modalFormEdit-{{ enseigne.id_enseigne }}" class="modal-with-form mb-xs mt-xs mr-xs btn btn-primary">
												<span class="fa fa-edit"></span>
													Editer
											</a>
											<!-- Modal Form Edition-->
											<div id="modalFormEdit-{{ enseigne.id_enseigne }}" class="modal-block modal-block-primary mfp-hide">
												<section class="panel">
													<header class="panel-heading">
														<h2 class="panel-title">Mise à jour de l'enseigne</h2>
													</header>
													<div class="panel-body">
														<form action="{{ path('edit_enseigne') }} " method="post">
															<input type="hidden" name="_token" value="{{ csrf_token('edit-item'~ app.user.id) }}"/>
															<input type="hidden" value="{{ enseigne.id_enseigne }}" name="id_enseigne">
															<div class="form-group mt-lg">
																<label class="col-sm-3 control-label">Nom de l'enseigne</label>
																<div class="col-sm-9">
																	<input type="text" name="nom_enseigne" class="form-control" placeholder="Entrez le nom de l'enseigne..." required value="{{enseigne.nom_enseigne }} " />
																</div>
															</div>
															<div class="form-group">
																<label class="col-sm-3 control-label">Code de l'enseigne</label>
																<div class="col-sm-9">
																	<input type="text" name="code_enseigne" class="form-control" placeholder="Entrez le code de l'enseigne..." required value="{{ enseigne.code_enseigne }} " />
																</div>
															</div>
															<div class="form-group text-right pr-md">
																<input type="submit" value="Mettre à jour" class="btn btn-primary">
															</div>
														</form>
													</div>
													<footer class="panel-footer">
														<div class="row">
															<div class="col-md-12 text-right">
																<button class="btn btn-default modal-dismiss">Cancel</button>
															</div>
														</div>
													</footer>
												</section>
											</div>
											{#<a href="{{ path('statistique', {'id_enseigne': enseigne.id_enseigne}) }}" class="mb-xs mt-xs mr-xs btn btn-default">
												<span class="fa fa-eye"></span>
												Dashboard
											</a>#}
												
										</td>
									</tr>
								{% else %}
									<tr class="gradeX" colspan="4"> Vous n'avez créé aucune enseigne</tr>
								{% endfor %}
							{% endif %}
						</tbody>
					</table>
				</div>
			</section>
			{% if app.user.type == TYPE_ENTREPRISE %}
				<section class="panel">
					<header class="panel-heading">
						<div class="panel-actions">
							<a href="#" class="fa fa-caret-down"></a>
						</div>
				
						<h2 class="panel-title">
							Enseignes de KhaMELIA
						</h2>
						
					</header>
					<div class="panel-body">
						<table class="table table-bordered table-striped mb-none" id="datatable-default">
							<thead>
								<tr>
									<th class="text-center">Nom de l'enseigne</th>
									<th class="text-center">Code de l'enseigne</th>
									<th class="text-center">Action(s)</th>
								</tr>
							</thead>
							<tbody class="text-center">
							{% for enseigne in enseignesAffiliees %}
								{% if enseigne.id not in myEnseignesId %}
									<tr class="gradeX center">
										<td>
											{{ enseigne.nomEnseigne }}
										</td>
										<td>
											{{ enseigne.codeEnseigne }}
										</td>
										<td class="center">
											{% if enseigne.id not in joined %}
												<a href="#modalCenter-{{ enseigne.id }}" class="modal-basic mb-xs mt-xs mr-xs btn btn-success">
													<span class="fa fa-reply "></span>
														Rejoindre
												</a>

												<div id="modalCenter-{{ enseigne.id }}" class="modal-block modal-block-primary mfp-hide">
													<section class="panel">
														<header class="panel-heading">
															<h2 class="panel-title">Attention</h2>
														</header>
														<div class="panel-body text-center">
															<div class="modal-wrapper">
																<div class="modal-icon center">
																	<i class="fa fa-question-circle"></i>
																</div>
																<div class="modal-text">
																	<h4>Attention</h4>
																	<p>Êtes-vous sur de vouloir rejoindre cette enseigne ?</p>
																</div>
															</div>
														</div>
														<footer class="panel-footer">
															<div class="row">
																<div class="col-md-12 text-right">
																	<a href="{{ path('rejoindre', {'id_enseigne': enseigne.id}) }}">
																		<button class="btn btn-primary">Oui</button>
																	</a>
																	<button class="btn btn-default modal-dismiss">Non</button>
																</div>
															</div>
														</footer>
													</section>
												</div>
											{% else %}

												{# Session for inscription if it's a secratire #}
												{% set isSecretaire = false %}
												{% set isInfirmier = false %}
												{% set isEnseignant = false %}
												{% set isEleve = false %}
												{% set isParent = false %}
												{% for value in onlineUserProfiles %}
													{% if value.IdEnseigne == enseigne.id %}
														{% if 'secretaire' in value.profiles %}  
															{% set isSecretaire = true %}
															<a href="{{ path('membre_inscription', {'id_enseigne': enseigne.id}) }}" class="mb-xs mt-xs mr-xs btn btn-info">
																<span class="fa fa-info"></span>
																Inscriptions
															</a>
														{% elseif 'infirmier' in value.profiles %}  
															{% set isInfirmier = true %}
															<a href="{{ path('constante_entente', {'id_enseigne': enseigne.id}) }}" class="mb-xs mt-xs mr-xs btn btn-info">
																<span class="fa fa-info"></span>
																Consultation
															</a>
														{% elseif 'enseignant' in value.profiles %}
															{% set isEnseignant = true %}
															<a href="{{ path('cours', {'id_enseigne': enseigne.id}) }}" class="mb-xs mt-xs mr-xs btn btn-default">
																<span class="fa fa-pencil"></span>
																Cours
															</a>
															<a href="{{ path('evaluation', {'id_enseigne': enseigne.id}) }}" class="mb-xs mt-xs mr-xs btn btn-default">
																<span class="fa fa-pencil"></span>
																Evaluations
															</a>
															<a href="#modalCenterTimeline-{{ enseigne.id }}" 
																class="mb-xs mt-xs mr-xs btn btn-primary modal-basic">
																<span class="fa fa-calendar"></span>
																Programme
															</a>
															<div id="modalCenterTimeline-{{ enseigne.id }}" 
																class="modal-block modal-block-primary mfp-hide ">
																<section class="panel">
																	<header class="panel-heading">
																		<h2 class="panel-title">Choix de la classe</h2>
																	</header>
																	<div class="panel-body">
																		{% if allClassesByEnseigne is not empty and allClassesByEnseigne[enseigne.id] is defined %}
																			<form action="{{ path('timeline', {'id_enseigne': enseigne.id}) }}" method="post">
																				<input type="hidden" name="_token" value="{{ csrf_token('timeline-consulte') }}" required/>
																				<label for="classe" classe="col-md-6">Classes</label>
																				<div class="form-group">
																					<div class="col-md-6">
																						<select class="form-control" name="classe" id="classe" required>
																							{% for item in allClassesByEnseigne[enseigne.id] %}
																								<option value="{{ item.id }}"> {{ item.libelle }} </option>
																							{% else %}
																								<option value=""> -- Aucune classe -- </option>
																							{% endfor %}
																						</select>
																					</div>
																				</div>
																				<div class="form-group mt-md">
																					<input type="submit" value="Consulter" class="btn btn-success">
																				</div>
																			</form>
																		{% else %}
																			<p>Cette enseigne ne dispose d'aucune classe</p>
																		{% endif %}
																	</div>
																	<footer class="panel-footer">
																		<div class="row">
																			<div class="col-md-12 text-right">
																				<button class="btn btn-default modal-dismiss">Fermer</button>
																			</div>
																		</div>
																	</footer>
																</section>
															</div>
															<a href="{{ path('evaluation', {'id_enseigne': enseigne.id}) }}" 
																class="mb-xs mt-xs mr-xs btn btn-danger">
																<span class="fa fa-calendar"></span>
																Evaluation
															</a>
														{% elseif 'eleve' in value.profiles  %}
															{% set isEleve = true %}
															<a href="#modalCenterTimeline-{{ enseigne.id }}" 
																class="mb-xs mt-xs mr-xs btn btn-primary modal-basic">
																<span class="fa fa-calendar"></span>
																Programme
															</a>
															<div id="modalCenterTimeline-{{ enseigne.id }}" 
																class="modal-block modal-block-primary mfp-hide">
																<section class="panel">
																	<header class="panel-heading">
																		<h2 class="panel-title">Choix de la classe</h2>
																	</header>
																	<div class="panel-body">
																		{% if allClassesByEnseigne is not empty and allClassesByEnseigne[enseigne.id] is defined %}
																			<form action="{{ path('timeline', {'id_enseigne': enseigne.id}) }}" method="post">
																				<input type="hidden" name="_token" value="{{ csrf_token('timeline-consulte') }}" required/>
																				<label for="classe" classe="col-md-6">Classes</label>
																				<div class="form-group">
																					<div class="col-md-6">
																						<select class="form-control" name="classe" id="classe" required>
																								{% for item in allClassesByEnseigne[enseigne.id] %}
																									<option value="{{ item.id }}"> {{ item.libelle }} </option>
																								{% else %}
																									<option value=""> -- Aucune classe -- </option>
																								{%endfor %}
																							
																						</select>
																					</div>
																				</div>
																				<div class="form-group mt-md">
																					<input type="submit" value="Consulter" class="btn btn-success">
																				</div>
																			</form>
																		{% else %}
																			<p>Cette enseigne ne dispose d'aucune classe</p>
																		{% endif %}
																	</div>
																	<footer class="panel-footer">
																		<div class="row">
																			<div class="col-md-12 text-right">
																				<button class="btn btn-default modal-dismiss">Fermer</button>
																			</div>
																		</div>
																	</footer>
																</section>
															</div>
														{% endif %}
													{% endif %}
												{% endfor %}

												{% set Ncoadmin = false %}
												{% for enseigneJoind in enseignesJoined %}
													{% if enseigneJoind.idUser == app.user.id and 'co_administrateur' in enseigneJoind.droits and enseigneJoind.idEnseigne == enseigne.id_enseigne %}
														<a href="{{ path('membre', {'id_enseigne': enseigne.id_enseigne}) }}" class="mb-xs mt-xs mr-xs btn btn-info">
															<span class="fa fa-users"></span>
															Membres
														</a>
														
														<a href="#modalFormType-{{ enseigne.id_enseigne }}" class="mb-xs mt-xs mr-xs btn btn-dark modal-with-form ">
															<span class="fa fa-cube"></span>
															Type enseigne
														</a>
														<div id="modalFormType-{{ enseigne.id_enseigne }}" class="modal-block modal-block-primary mfp-hide">
															<section class="panel">
																<header class="panel-heading">
																	<h2 class="panel-title">Ajout de type d'enseigne</h2>
																</header>
																<div class="panel-body">
