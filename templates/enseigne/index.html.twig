															Spécialités
														</a>
														{% set Ncoadmin = true %}
													
													{% endif %}
													
												{% endfor %}
												
												{# if Ncoadmin == false and isSecretaire == false and isInfirmier == false and isEnseignant == false and isEleve == false %}
													Aucune(s)
												{% endif #}
												<a href="{{ path('statistique', {'id_enseigne': enseigne.id}) }}" class="mb-xs mt-xs mr-xs btn btn-default">
													<span class="fa fa-eye"></span>
													Dashboard
												</a>
											{% endif %}
										</td>
									</tr>
								{% endif %}
							{% endfor %}
							</tbody>
						</table>
					</div>
				</section>
			{% endif %}

			
		</div>
	</div>
